<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pemrograman_android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/chefhat"
        android:label="@string/app_name"
        android:roundIcon="@drawable/chefhat_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pemrograman_android.ResepDetailActivity"
            android:label="Detail Resep"
            android:parentActivityName="com.example.pemrograman_android.MainActivity"></activity>
    </application>

</manifest>